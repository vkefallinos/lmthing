{
  "name": "lmthing",
  "version": "0.1.0",
  "description": "A library for creating AI agents with structured prompts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "lmthing": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc && npm run build:cli",
    "build:cli": "esbuild src/cli.ts --bundle --platform=node --format=esm --outfile=dist/cli.js --packages=external",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "ai",
    "agent",
    "testing",
    "mock",
    "ai-sdk"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^3.0.67",
    "@ai-sdk/anthropic": "^2.0.53",
    "@ai-sdk/azure": "^2.0.79",
    "@ai-sdk/cohere": "^2.0.20",
    "@ai-sdk/google": "^2.0.44",
    "@ai-sdk/google-vertex": "^3.0.86",
    "@ai-sdk/groq": "^2.0.32",
    "@ai-sdk/mistral": "^2.0.25",
    "@ai-sdk/openai": "^2.0.77",
    "ai": "^5.0.0",
    "js-yaml": "^4.1.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "esbuild": "^0.27.1",
    "msw": "^2.12.4",
    "typescript": "^5.5.4",
    "vitest": "^4.0.15"
  }
}
